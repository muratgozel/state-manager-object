{
  "name": "state-manager-object",
  "version": "0.1.1",
  "description": "State management for javascript modules, libraries and apps.",
  "main": "dist/index.js",
  "scripts": {
    "release": "bash release/release.sh",
    "builds": "npm run build && npm run build-umd && npm run build-umd-polyfilled",
    "build": "BABEL_ENV=development ./node_modules/.bin/minify ./source -d ./dist && ls -lR ./dist",
    "build-umd": "BABEL_ENV=development ./node_modules/.bin/browserify -s StateManagerObject ./source/index.js -o ./dist/umd.js && ./node_modules/.bin/minify ./dist/umd.js --out-file ./dist/umd.js",
    "build-umd-polyfilled": "mkdir -p ./source-compiled && BABEL_ENV=production ./node_modules/.bin/babel ./source -d ./source-compiled && ./node_modules/.bin/browserify -s StateManagerObject ./source-compiled/index.js -o ./dist/umd.polyfilled.js && ./node_modules/.bin/minify ./dist/umd.polyfilled.js --out-file ./dist/umd.polyfilled.js && rm -rf ./source-compiled",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muratgozel/state-manager-object.git"
  },
  "keywords": [
    "state",
    "javascript"
  ],
  "author": "Murat GÃ¶zel <murat@gozel.com.tr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muratgozel/state-manager-object/issues"
  },
  "homepage": "https://github.com/muratgozel/state-manager-object#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-minify": "^0.5.0",
    "babel-preset-minify": "^0.5.0",
    "browserify": "^16.2.3",
    "core-js": "^3.1.3"
  },
  "dependencies": {
    "event-emitter-object": "^0.1.8",
    "my-little-lodash": "^0.1.1"
  }
}